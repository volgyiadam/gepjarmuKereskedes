<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="gepjarmuKereskedes" namespace="gepjarmuKereskedes.Models">
  <class name="Car" table="Car" dynamic-update="true" lazy="true" >
    <cache usage="read-write"/>
    <id name="Id" access="property" column="Id" type="Int32">
      <generator class="native" />
    </id>
    <property name="Brand" access="property" column="Brand" type="String" />
    <property name="Type" access="property" column="Type" type="String" />
    <property name="Vintage" access="property" column="Vintage" type="Int32" />
    <property name="ProductionTime" access="property" column="ProductionTime" type="DateTime" />
    <property name="Condition" access="property" column="Condition" type="String" />
    <property name="PreviousOwners" access="property" column="PreviousOwners" type="Int32" />    
    <property name="SiteId" access="property" column="SiteId" type="Int32" />
    
  </class>
</hibernate-mapping>
